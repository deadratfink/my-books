{
  "source": {
    "include": [
      "src",
      "test"
    ],
    "exclude": [
      "node_modules"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)(_|\\.)"
  },
  "opts": {
    "encoding": "utf8",
    "recurse": true
  },
  "plugins": [
    "node_modules/jsdoc-babel"
  ],
  "babel": {
    "plugins": [
      "@babel/transform-async-to-generator",
      "@babel/proposal-class-properties",
      "@babel/proposal-object-rest-spread",
      "@babel/transform-react-jsx"
    ]
  }
}
